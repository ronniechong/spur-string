{
  "name": "spur-string",
  "version": "0.0.2",
  "description": "String utilities to be used with the Spur Framework.",
  "main": "dist/SpurString.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "babel --watch src -d dist",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "babel-node --debug node_modules/mocha/bin/_mocha ./test/",
    "precoveralls": "npm run lint",
    "coveralls": "babel-node node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha --report lcovonly ./test/ && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/opentable/spur-string.git"
  },
  "keywords": [
    "spur",
    "spur-framework",
    "util",
    "string"
  ],
  "author": {
    "name": "Agustin Colchado",
    "email": "agustin@colchado.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opentable/spur-string/issues"
  },
  "homepage": "https://github.com/opentable/spur-string",
  "dependencies": {
    "require-all": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.4.0",
    "eslint-config-opentable": "^2.0.1",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
